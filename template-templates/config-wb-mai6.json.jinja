                "enum_titles": [
                    "Disabled",
                    "Thermocouple type K (ТХА)",

                    "P: 2-wire RTD Pt 50 (α = 0.00385 °C⁻¹)",
                    "P: 2-wire RTD Pt 100 (α = 0.00385 °C⁻¹)",
                    "P: 2-wire RTD Pt 500 (α = 0.00385 °C⁻¹)",
                    "P: 2-wire RTD Pt 1000 (α = 0.00385 °C⁻¹)",
                    "P: 2-wire RTD 50P (α = 0.00391 °C⁻¹)",
                    "P: 2-wire RTD 100P (α = 0.00391 °C⁻¹)",
                    "P: 2-wire RTD 500P (α = 0.00391 °C⁻¹)",
                    "P: 2-wire RTD 1000P (α = 0.00391 °C⁻¹)",
                    "P: 2-wire RTD 50M (α = 0.00428 °C⁻¹)",
                    "P: 2-wire RTD 100M (α = 0.00428 °C⁻¹)",
                    "P: 2-wire RTD 500M (α = 0.00428 °C⁻¹)",
                    "P: 2-wire RTD 1000M (α = 0.00428 °C⁻¹)",
                    "P: 2-wire RTD Ni 100 (α = 0.00617 °C⁻¹)",
                    "P: 2-wire RTD Ni 500 (α = 0.00617 °C⁻¹)",
                    "P: 2-wire RTD Ni 1000 (α = 0.00617 °C⁻¹)",

                    "3-wire RTD Pt 50 (α = 0.00385 °C⁻¹)",
                    "3-wire RTD Pt 100 (α = 0.00385 °C⁻¹)",
                    "3-wire RTD Pt 500 (α = 0.00385 °C⁻¹)",
                    "3-wire RTD Pt 1000 (α = 0.00385 °C⁻¹)",
                    "3-wire RTD 50P (α = 0.00391 °C⁻¹)",
                    "3-wire RTD 100P (α = 0.00391 °C⁻¹)",
                    "3-wire RTD 500P (α = 0.00391 °C⁻¹)",
                    "3-wire RTD 1000P (α = 0.00391 °C⁻¹)",
                    "3-wire RTD 50M (α = 0.00428 °C⁻¹)",
                    "3-wire RTD 100M (α = 0.00428 °C⁻¹)",
                    "3-wire RTD 500M (α = 0.00428 °C⁻¹)",
                    "3-wire RTD 1000M (α = 0.00428 °C⁻¹)",
                    "3-wire RTD Ni 100 (α = 0.00617 °C⁻¹)",
                    "3-wire RTD Ni 500 (α = 0.00617 °C⁻¹)",
