                    "P: dry contact sensor",
                    "P: NTC thermistor 10k (B = 3988 K)",

                    "P: voltage measurement from ratiometric sensor",
                    "P: single-ended voltage measurement from 0 to 30 V",
                    "P: two-wire resistance measurement",
                    "P: current measurement",
                    "voltage measurement in differential mode from -30 to 30 V",
                    "three-wire resistance measurement"
                ]
            },
            "in{{ ch_num }}_data_rate": {
                "title": "Sampling time",
                "address": "0x{{ "%x"|format(ch_num) }}402",
                "group": "g_in{{ ch_num }}",
                "order": 2,
                "condition": "in{{ ch_num }}_type!=65535",
                "enum_titles": [
                    "50 ms (20 SPS)",
                    "22 ms (45 SPS)",
                    "11 ms (90 SPS)",
                    "5.7 ms (175 SPS)",
                    "3.0 ms (330 SPS)",
                    "1.7 ms (600 SPS)",
                    "1.0 ms (1000 SPS)"
