                "title": "Mode",
                "address": "0x{{ "%x"|format(ch_num) }}400",
                "group": "g_in{{ ch_num }}",
                "order": 1,
                "required": true,
                "default": 65535,
                "enum": [
                    65535,

                    4096,

                    4352,
                    4353,
                    4354,
                    4355,
                    4368,
                    4369,
                    4370,
                    4371,
                    4384,
                    4385,
                    4386,
                    4387,
                    4400,
                    4401,
                    4402,

                    4608,
                    4609,
                    4610,
                    4611,
                    4624,
                    4625,
                    4626,
                    4627,
                    4640,
                    4641,
                    4642,
                    4643,
                    4656,
                    4657,
                    4658,

                    4864,
                    4865,
                    4866,

                    5120,
                    5376,

                    5632,
                    5889,

                    0,
                    1,
                    2,
                    3,
                    257,
                    258
