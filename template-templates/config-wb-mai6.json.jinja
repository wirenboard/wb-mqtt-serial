                "format": "s16",
                "address": "0x{{ "%x"|format(ch_num) }}504",
                "group": "g_in{{ ch_num }}",
                "condition": "in{{ ch_num }}_type==4096||(in{{ ch_num }}_type>=4608&&in{{ ch_num }}_type<=4658)"
            },
            {
                "name": "IN {{ch_num}} P Temperature",
