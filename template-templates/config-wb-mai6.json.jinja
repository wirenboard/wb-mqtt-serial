{% set CHANNELS_NUMBER = 6 -%}
{%- set types_with_gain = [0, 1, 3, 257] -%}
{%- set types_with_min_max = [ 5376, 4864, 4865, 4866, 0, 5120] -%}
{%- set types_with_single_value_channel = [ 4864, 4865, 4866, 5120] -%}
{%- set single_ended_modes =  [0, 1, 2, 3, 4352, 4353, 4354, 4355, 4368, 4369, 4370, 4371, 4384, 4385, 4386, 4387, 4400, 4401, 4402, 4864, 4865, 4866, 5120, 5632, 5889, 65535 ] -%}
{%- macro format_list(index, types, format) -%}
    {% set separator = joiner('||') %}
    {%- for t in types -%}{{ separator() }}{{ format|format(index, t) }}{%- endfor -%}
{%- endmacro -%}
{
    "title": "WB-MAI6_template_title",
    "device_type": "WB-MAI6",
    "group": "g-wb",
    "device": {
        "name": "WB-MAI6",
        "id": "wb-mai6",
        "max_read_registers": 4,
        "frame_timeout_ms": 0,
        "response_timeout_ms": 1,

        "groups": [
            {% for ch_num in range(1, CHANNELS_NUMBER + 1) -%}
            {
                "title": "IN {{ ch_num }}",
                "id": "g_in{{ ch_num }}",
                "order": {{ ch_num }}
            },
            {
                "title": "IN_N_title",
                "id": "gg_in{{ ch_num }}_n",
                "order": {{ ch_num }},
                "group": "g_in{{ ch_num }}"
            },
            {% endfor -%}
            {
                "title": "General",
                "id": "general",
                "order": 7
            },
            {
                "title": "HW Info",
                "id": "hw_info",
                "order": 8
            },
            {
                "title": "Debug",
                "id": "debug",
                "order": 9
            }
        ],

        "parameters": {
            {% for ch_num in range(1, CHANNELS_NUMBER + 1) -%}
