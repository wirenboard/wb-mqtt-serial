            {
                "name": "IN {{ch_num}} N Resistance",
                "type": "value",
                "units": "Ohm",
                "error_value": "0x7FFFFFFF",
                "reg_type": "input",
                "format": "s32",
                "address": "0x{{ "%x"|format(ch_num) }}502",
                "scale": 0.01,
                "condition": "in{{ ch_num }}_n_type==2||in{{ ch_num }}_n_type==5632||in{{ ch_num }}_n_type==5889||(in{{ ch_num }}_n_type>=4352&&in{{ ch_num }}_n_type<=4402)",
                "group": "gg_in{{ ch_num }}_n"
            },
            {
                "name": "IN {{ch_num}} N Temperature",
                "type": "value",
                "units": "deg C",
