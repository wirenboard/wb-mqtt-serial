            },
            {
                "name": "IN {{ch_num}} Value",
                "reg_type": "input",
                "type": "value",
                "format": "s16",
                "error_value": "0x7fff",
                "address": "0x{{ "%x"|format(ch_num) }}504",
                "group": "g_in{{ ch_num }}",
                "condition": "in{{ ch_num }}_type==5376"
            },
            {
                "name": "IN {{ch_num}} P Value",
                "reg_type": "input",
                "type": "value",
                "format": "s16",
                "error_value": "0x7fff",
                "address": "0x{{ "%x"|format(ch_num) }}504",
                "group": "g_in{{ ch_num }}",
                "condition": "{{ format_list(ch_num, types_with_single_value_channel, "in%s_type==%s") }}"
