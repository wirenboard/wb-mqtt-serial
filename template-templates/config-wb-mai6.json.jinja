                "title": "Lowpass filter time, ms",
                "group": "g_in{{ ch_num }}",
                "order": 4,
                "address": "0x{{ "%x"|format(ch_num) }}406",
                "min": 0,
                "max": 65000,
                "condition": "in{{ ch_num }}_type!=65535"
            },
            "in{{ch_num}}_gain": {
                "title": "Gain",
                "group": "g_in{{ ch_num }}",
                "order": 4,
                "address": "0x{{ "%x"|format(ch_num) }}40C",
                "enum": [0, 1, 2, 4],
                "enum_titles": ["auto"],
                "default": 0,
                "condition": "{{ format_list(ch_num, types_with_gain, "in%s_type==%s") }}"
            },
            "in{{ ch_num }}_min": {
                "title": "Minimum value",
                "group": "g_in{{ ch_num }}",
                "order": 5,
                "address": "0x{{ "%x"|format(ch_num) }}408",
                "default": 0,
                "condition": "{{ format_list(ch_num, types_with_min_max, "in%s_type==%s") }}"
            },
            "in{{ ch_num }}_max": {
                "title": "Maximum value",
                "group": "g_in{{ ch_num }}",
                "order": 6,
                "address": "0x{{ "%x"|format(ch_num) }}40A",
                "default": 10000,
                "condition": "{{ format_list(ch_num, types_with_min_max, "in%s_type==%s") }}"
            },
            "in{{ ch_num }}_n_type": {
                "title": "Mode",
                "address": "0x{{ "%x"|format(ch_num) }}401",
                "group": "gg_in{{ ch_num }}_n",
                "order": 1,
                "required": true,
                "default": 65535,
                "condition": "{{ format_list(ch_num, single_ended_modes, "in%s_type==%s") }}",
