                    4352,
                    4353,
                    4354,
                    4355,
                    4368,
                    4369,
                    4370,
                    4371,
                    4384,
                    4385,
                    4386,
                    4387,
                    4400,
                    4401,
                    4402,

                    4864,
                    4865,
                    4866,

                    5120,
                    5376,

                    5632,
                    5889,

                    0,
                    1,
                    2,
                    3
                ],
                "enum_titles": [
                    "Disabled",

                    "2-wire RTD Pt 50 (α = 0.00385 °C⁻¹)",
                    "2-wire RTD Pt 100 (α = 0.00385 °C⁻¹)",
                    "2-wire RTD Pt 500 (α = 0.00385 °C⁻¹)",
                    "2-wire RTD Pt 1000 (α = 0.00385 °C⁻¹)",
                    "2-wire RTD 50P (α = 0.00391 °C⁻¹)",
                    "2-wire RTD 100P (α = 0.00391 °C⁻¹)",
                    "2-wire RTD 500P (α = 0.00391 °C⁻¹)",
                    "2-wire RTD 1000P (α = 0.00391 °C⁻¹)",
                    "2-wire RTD 50M (α = 0.00428 °C⁻¹)",
                    "2-wire RTD 100M (α = 0.00428 °C⁻¹)",
                    "2-wire RTD 500M (α = 0.00428 °C⁻¹)",
                    "2-wire RTD 1000M (α = 0.00428 °C⁻¹)",
                    "2-wire RTD Ni 100 (α = 0.00617 °C⁻¹)",
                    "2-wire RTD Ni 500 (α = 0.00617 °C⁻¹)",
                    "2-wire RTD Ni 1000 (α = 0.00617 °C⁻¹)",

                    "0-5 mA sensor",
                    "0-20 mA sensor",
                    "4-20 mA sensor",

                    "0-1 V sensor",
                    "-50 to 50 mV sensor",

                    "dry contact sensor",
                    "NTC thermistor 10k (B = 3988 K)",

                    "N: voltage measurement from ratiometric sensor",
                    "N: single-ended voltage measurement from 0 to 30 V",
                    "N: two-wire resistance measurement",
                    "N: current measurement"
                ]
            },
            "in{{ ch_num }}_n_data_rate": {
                "title": "Sampling time",
                "group": "gg_in{{ ch_num }}_n",
                "order": 2,
                "address": "0x{{ "%x"|format(ch_num) }}403",
                "enum_titles": [
                    "50 ms (20 SPS)",
                    "22 ms (45 SPS)",
                    "11 ms (90 SPS)",
                    "5.7 ms (175 SPS)",
                    "3.0 ms (330 SPS)",
                    "1.7 ms (600 SPS)",
                    "1.0 ms (1000 SPS)"
                ],
                "enum": [ 20, 45, 90, 175, 330, 600, 1000 ],
                "condition": "in{{ ch_num }}_n_type!=65535"
            },
            "in{{ ch_num }}_n_measurements": {
                "title": "Number of measurements",
                "description": "Total sampling time of channel is (Sampling time * Number of measurements). Channels are polled sequentially.",
                "group": "gg_in{{ ch_num }}_n",
                "order": 3,
                "address": "0x{{ "%x"|format(ch_num) }}405",
                "min": 1,
                "max": 50,
                "condition": "in{{ ch_num }}_n_type!=65535"
            },
            "in{{ ch_num }}_n_lowpass": {
                "title": "Lowpass filter time, ms",
                "group": "gg_in{{ ch_num }}_n",
                "order": 4,
                "address": "0x{{ "%x"|format(ch_num) }}407",
                "min": 0,
                "max": 65000,
                "condition": "in{{ ch_num }}_n_type!=65535"
            },
            "in{{ch_num}}_n_gain": {
                "title": "Gain",
                "group": "gg_in{{ ch_num }}_n",
                "order": 4,
                "address": "0x{{ "%x"|format(ch_num) }}40D",
                "enum": [0, 1, 2, 4],
                "enum_titles": ["auto"],
                "default": 0,
                "condition": "{{ format_list(ch_num, types_with_gain, "in%s_n_type==%s") }}"
            },
            "in{{ ch_num }}_n_min": {
                "title": "Minimum value",
                "group": "gg_in{{ ch_num }}_n",
                "order": 5,
                "address": "0x{{ "%x"|format(ch_num) }}409",
                "default": 0,
                "condition": "{{ format_list(ch_num, types_with_min_max, "in%s_n_type==%s") }}"
            },
            "in{{ ch_num }}_n_max": {
                "title": "Maximum value",
                "group": "gg_in{{ ch_num }}_n",
                "order": 6,
                "address": "0x{{ "%x"|format(ch_num) }}40B",
                "default": 10000,
                "condition": "{{ format_list(ch_num, types_with_min_max, "in%s_n_type==%s") }}"
            },
            {% endfor -%}
            "baud_rate": {
                "title": "Baud rate",
                "description": "baud_rate_description",
                "address": 110,
                "reg_type": "holding",
                "enum": [96, 192, 384, 576, 1152],
                "default": 96,
                "enum_titles": [
                    "9600",
                    "19200",
                    "38400",
                    "57600",
                    "115200"
                ],
                "group": "general",
                "order": 1
            }
        },

        "channels": [
            {% for ch_num in range(1, CHANNELS_NUMBER + 1) -%}
            {
                "name": "IN {{ch_num}} Resistance",
                "type": "value",
                "units": "Ohm",
                "error_value": "0x7FFFFFFF",
                "reg_type": "input",
                "format": "s32",
                "address": "0x{{ "%x"|format(ch_num) }}500",
                "scale": 0.01,
                "group": "g_in{{ ch_num }}",
                "condition": "(in{{ ch_num }}_type>=4608&&in{{ ch_num }}_type<=4658)||in{{ ch_num }}_type==258"
            },
            {
                "name": "IN {{ch_num}} P Resistance",
                "type": "value",
                "units": "Ohm",
                "error_value": "0x7FFFFFFF",
                "reg_type": "input",
                "format": "s32",
                "address": "0x{{ "%x"|format(ch_num) }}500",
                "scale": 0.01,
                "group": "g_in{{ ch_num }}",
                "condition": "in{{ ch_num }}_type==2||(in{{ ch_num }}_type>=4352&&in{{ ch_num }}_type<=4402)||in{{ ch_num }}_type==5632||in{{ ch_num }}_type==5889"
