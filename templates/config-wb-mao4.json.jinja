{% set title_en = title_en | default("WB-MAO4 (4-channel analog output module)") -%}
{% set title_ru = title_ru | default("WB-MAO4 (4-канальный модуль аналогового вывода)") -%}
{% set device_type = device_type | default("tpl1_wb_mao4") -%}
{% set group = group | default("g-wb") -%}
{% set device_name = device_name | default("WB-MAO4") -%}
{% set device_id = device_id | default("wb-mao4") -%}
{% set has_signature = has_signature | default(true) -%}

{% set INPUTS_NUMBER = 3 -%}
{% set OUTPUTS_NUMBER = 4 -%}
{% set PRESS_TYPES = ["short", "long", "double", "short long"] %}
{% set LIGHT_SCENES_NUMBER = 8 -%}

{
    "title": "template_title",
    "device_type": "{{ device_type }}",
    "group": "{{ group }}",
{% if has_signature %}
    "hw": [
        {
            "signature": "WBMAO4"
        }
    ],
{% endif %}
    "device": {
        "name": "{{ device_name }}",
        "id": "{{ device_id }}",
        "max_read_registers": 0,
        "response_timeout_ms": 1,
        "frame_timeout_ms": 8,
        "enable_wb_continuous_read": true,

        "groups": [
            {% for ch_number in range(1, INPUTS_NUMBER + 1) -%}
            {
                "title": "Input {{ ch_number }}",
                "id": "input_{{ ch_number }}"
            },
            {% for press_type in PRESS_TYPES -%}
            {
                "title": "{{ press_type | title }} Press Action",
                "id": "input_{{ ch_number }}_{{ press_type | replace(' ', '_') }}_press_action",
                "group": "input_{{ ch_number }}"
            },
            {% endfor -%}
            {
                "title": "Input Parameters",
                "id": "input_{{ ch_number }}_parameters",
                "group": "input_{{ch_number }}",
                "ui_options": {
                    "wb": {
                        "disable_title": true
                    }
                }
            },
            {
                "title": "Input Counters",
                "id": "input_{{ ch_number }}_counters",
                "group": "input_{{ ch_number }}",
                "ui_options": {
                    "wb": {
                        "disable_title": true
                    }
                }
            },
            {% endfor -%}
            {
                "title": "Outputs",
                "id": "outputs"
            },
            {
                "title": "Outputs Common Settings",
                "id": "outputs_common",
                "group": "outputs"
            },
            {% for ch_number in range(1, OUTPUTS_NUMBER + 1) -%}
            {
                "title": "Output {{ ch_number }}",
                "id": "output{{ ch_number }}",
                "group": "outputs"
            },
            {% endfor -%}
            {
                "title": "Output Channels",
                "id": "output_channels",
                "group": "outputs",
                "ui_options": {
                    "wb": {
                        "disable_title": true
                    }
                }
            },
            {% for ls_number in range(1, LIGHT_SCENES_NUMBER + 1) -%}
            {
                "title": "Light Scene {{ ls_number }}",
                "id": "g_light_scene_{{ ls_number }}",
                "description": "light_scenes_description"
            },
            {
                "title": "Actions if Light is Off",
                "id": "gg_ls_{{ ls_number }}_actions_off",
                "group": "g_light_scene_{{ ls_number }}"
            },
            {
                "title": "Actions if Light is On",
                "id": "gg_ls_{{ ls_number }}_actions_on",
                "group": "g_light_scene_{{ ls_number }}"
            },
            {
                "title": "Light Scene {{ ls_number }} Channels",
                "id": "gg_ls_{{ ls_number }}_channels",
                "group": "g_light_scene_{{ ls_number }}",
                "ui_options": {
                    "wb": {
                        "disable_title": true
                    }
                }
            },
            {% for ch_number in range(1, OUTPUTS_NUMBER + 1) -%}
            {
                "title": "Channel {{ ch_number }} Actions",
                "id": "gg_ls_{{ ls_number }}_ch_{{ ch_number }}_action_off",
                "group": "gg_ls_{{ ls_number }}_actions_off"
            },
            {
                "title": "Channel {{ ch_number }} Actions",
                "id": "gg_ls_{{ ls_number }}_ch_{{ ch_number }}_action_on",
                "group": "gg_ls_{{ ls_number }}_actions_on"
            },
            {% endfor -%}
            {% endfor -%}

            {
                "title": "Safety Mode",
                "id": "g_safety",
                "description": "g_safety_description"
            },
            {
                "title": "Power Up Behaviour",
                "id": "g_power_up_behaviour",
                "description": "g_power_up_behaviour_description"
            },
            {
                "title": "Safety Behaviour",
                "id": "gg_safety_behaviour",
                "description": "safety_behaviour_description",
                "group": "g_safety"
            },
            {% for ch_number in range(1, OUTPUTS_NUMBER + 1) -%}
            {
                "title": "Channel {{ ch_number }}",
                "id": "gg_safete_mode_ch_{{ ch_number }}",
                "group": "gg_safety_behaviour",
                "ui_options": {
                    "wb": {
                        "disable_title": true
                    }
                }
            },
            {
                "title": "Channel {{ ch_number }}",
                "id": "gg_power_up_behaviour_ch_{{ ch_number }}",
                "group": "g_power_up_behaviour"
            },
            {% endfor -%}
            {
                "title": "Inputs Control In Safety Mode",
                "id": "gg_safety_input_control",
                "description": "inputs_control_in_safety_mode_description",
                "group": "g_safety"
            },

            {
                "title": "General",
                "id": "general"
            },
            {
                "title": "HW Info",
                "id": "hw_info"
            },
            {
                "title": "Debug",
                "id": "debug"
            }
        ],
        "parameters": [
            {
                "id": "outputs_mode",
                "title": "Outputs Mode",
                "address": 10,
                "reg_type": "holding",
                "enum": [0, 1],
                "enum_titles": [
                    "PWM 10 V",
                    "Analog 0-10 V"
                ],
                "default": 1,
                "group": "outputs_common",
                "order": 1
            },
            {
                "id": "pwm_frequency_factor",
                "title": "PWM Frequency Factor",
                "description": "pwm_frequency_factor_description",
                "address": 9,
                "reg_type": "holding",
                "default": 24,
                "min": 1,
                "max": 240,
                "group": "outputs_common",
                "order": 2
            },
            {% for ch_number in range(1, OUTPUTS_NUMBER + 1) -%}
            {
                "id": "output{{ ch_number }}_min",
                "title": "Voltage at 1% dimming level (mV)",
                "description": "level_1_voltage_description",
                "group": "output{{ ch_number }}",
                "address": {{ 912 + ch_number - 1 }},
                "reg_type": "holding",
                "default": 1,
                "min": 0,
                "max": 10000,
                "order": 1
            },
            {
                "id": "output{{ ch_number }}_max",
                "title": "Voltage at 100% dimming level (mV)",
                "description": "level_100_voltage_description",
                "group": "output{{ch_number}}",
                "address": {{928 + ch_number - 1}},
                "reg_type": "holding",
                "default": 10000,
                "min": 0,
                "max": 10000,
                "order": 2
            },
            {
                "id": "output{{ ch_number }}_decrement_rate",
                "title": "Dimming level decrement rate (ms/%)",
                "description": "increase_rate_description",
                "group": "output{{ch_number}}",
                "address": {{944 + ch_number - 1}},
                "reg_type": "holding",
                "default": 20,
                "min": 0,
                "max": 500,
                "order": 3
            },
            {
                "id": "channel{{ ch_number }}_increment_rate",
                "title": "Dimming level increment rate (ms/%)",
                "description": "decrease_rate_description",
                "group": "output{{ch_number}}",
                "address": {{960 + ch_number - 1}},
                "reg_type": "holding",
                "default": 20,
                "min": 0,
                "max": 500,
                "order": 4
            },
            {
                "id": "action{{ch_number}}_repeat_time",
                "title": "Action repeat time (ms)",
                "description": "action_repeat_time_description",
                "group": "output{{ch_number}}",
                "address": {{896 + ch_number - 1}},
                "reg_type": "holding",
                "default": 20,
                "min": 5,
                "max": 500,
                "order": 5
            },
            {% endfor -%}
            {% for ch_number in range(0, INPUTS_NUMBER) -%}
            {
                "id": "input_{{ch_number + 1}}_hold_time",
                "title": "Long Press Time (ms)",
                "description": "input_hold_time_description",
                "address": {{1100 + ch_number}},
                "reg_type": "holding",
                "default": 1000,
                "min": 500,
                "max": 5000,
                "group": "input_{{ch_number + 1}}_parameters",
                "order": 1
            },
            {
                "id": "input_{{ch_number + 1}}_double_time",
                "title": "Second Press Waiting Time (ms)",
                "description": "input_double_time_description",
                "address": {{1140 + ch_number}},
                "reg_type": "holding",
                "default": 300,
                "min": 0,
                "max": 2000,
                "group": "input_{{ch_number + 1}}_parameters",
                "order": 2
            },
            {
                "id": "input_{{ch_number + 1}}_debounce_time",
                "title": "Debounce Time (ms)",
                "description": "input_debounce_time_description",
                "address": {{1160 + ch_number}},
                "reg_type": "holding",
                "default": 50,
                "min": 5,
                "max": 200,
                "group": "input_{{ch_number + 1}}_parameters",
                "order": 3
            },
            {% for press_type in PRESS_TYPES -%}
            {% set action_bind = ("input {} {} press output".format(ch_number + 1, press_type))|replace(' ', '_')  %}
            {
                "id": "{{action_bind}}_number",
                "title": "Output Number",
                "address": {{768 + (loop.index0 * 16) + ch_number}},
                "reg_type": "holding",
                "enum": [0, 1, 2, 3, 4, 100, 101],
                "enum_titles": [
                    "No Action",
                    "Output 1",
                    "Output 2",
                    "Output 3",
                    "Output 4",
                    "All Outputs",
                    "Scene Mode"
                ],
                "default": 0,
                "group": "input_{{ch_number + 1}}_{{press_type | replace(' ', '_')}}_press_action",
                "order": 1
            },
            {
                "id": "{{action_bind}}_action_single",
                "title": "Action",
                "address": {{832 + (loop.index0 * 16) + ch_number}},
                "reg_type": "holding",
                {% if press_type in ["long", "short long"] %}"enum": [0, 1, 2, 3, 4, 5],{% else %}"enum": [0, 1, 2, 3, 4],{% endif %}
                "enum_titles": [
                    "Turn Off",
                    "Turn On",
                    "Toggle",
                    "Decrease level",
                    "Increase level"{% if press_type in ["long", "short long"] %},
                    "Increase/Decrease level"{% endif %}
                ],
                "default": 0,
                "group": "input_{{ch_number + 1}}_{{press_type | replace(' ', '_')}}_press_action",
                "order": 2,
                "condition": "({{action_bind}}_number>0)&&({{action_bind}}_number<100)"
            },
            {
                "id": "{{action_bind}}_action_all",
                "title": "Action",
                "address": {{832 + (loop.index0 * 16) + ch_number}},
                "reg_type": "holding",
                "enum": [0, 1, 3, 4],
                "enum_titles": [
                    "Turn Off",
                    "Turn On",
                    "Increase level",
                    "Decrease level"
                ],
                "default": 0,
                "group": "input_{{ch_number + 1}}_{{press_type | replace(' ', '_') }}_press_action",
                "order": 2,
                "condition": "{{ action_bind }}_number==100"
            },
            {
                "id": "{{action_bind}}_action_scene",
                "title": "Scene",
                "address": {{832 + (loop.index0 * 16) + ch_number}},
                "reg_type": "holding",
                "enum": [{% for ls_number in range(1, LIGHT_SCENES_NUMBER + 1) %}{{ ls_number }}{% if not loop.last %},{% endif %}{% endfor %}],
                "enum_titles": [
                    {% for ls_number in range(1, LIGHT_SCENES_NUMBER + 1) -%}
                    "Set Scene {{ls_number}}"{% if not loop.last %},{% endif %}
                    {% endfor -%}
                ],
                "group": "input_{{ch_number + 1}}_{{press_type | replace(' ', '_') }}_press_action",
                "order": 2,
                "required": true,
                "condition": "{{ action_bind }}_number==101"
            },
            {% endfor -%}{% endfor -%}

            {% for ls_number in range(1, LIGHT_SCENES_NUMBER + 1) -%}
            {
                "id": "ls_{{ls_number}}_light_on_off_condition",
                "title": "Light On/Off Condition",
                "reg_type": "holding",
                "address": {{1280 + (ls_number - 1)}},
                "type": "enum",
                "enum": [0, 1],
                "enum_titles": [
                    "Light is on if any affected channel is on. Otherwise light is off",
                    "Light is on if all affected channels are on. Otherwise light is off"
                ],
                "group": "g_light_scene_{{ ls_number }}"
            },
            {% for ch_number in range(1, OUTPUTS_NUMBER + 1) -%}
            {
                "id": "ls_{{ls_number}}_ch_{{ch_number}}_action_off",
                "title": "Action",
                "reg_type": "holding",
                "address": {{1344 + (ls_number - 1) * 8 + (ch_number - 1)}},
                "type": "enum",
                "enum": [0, 2, 3],
                "enum_titles": [
                    "Channel not Affected by Scene",
                    "Turn On",
                    "Turn On and Set Brightness"
                ],
                "group": "gg_ls_{{ls_number}}_ch_{{ch_number}}_action_off"
            },
            {
                "id": "ls_{{ls_number}}_ch_{{ch_number}}_fading_off",
                "title": "Dimming level rate (ms/%)",
                "reg_type": "holding",
                "address": {{1408 + (ls_number - 1) * 8 + (ch_number - 1)}},
                "max": 100,
                "required": true,
                "group": "gg_ls_{{ls_number}}_ch_{{ch_number}}_action_off",
                "condition": "ls_{{ls_number}}_ch_{{ch_number}}_action_off>=1"
            },
            {
                "id": "ls_{{ls_number}}_ch_{{ch_number}}_level_off",
                "title": "Brightness",
                "reg_type": "holding",
                "address": {{1472 + (ls_number - 1) * 8 + (ch_number - 1)}},
                "min": 1,
                "max": 100,
                "required": true,
                "group": "gg_ls_{{ls_number}}_ch_{{ch_number}}_action_off",
                "condition": "ls_{{ls_number}}_ch_{{ch_number}}_action_off==3"
            },
            {
                "id": "ls_{{ls_number}}_ch_{{ch_number}}_action_on",
                "title": "Action",
                "reg_type": "holding",
                "address": {{1536 + (ls_number - 1) * 8 + (ch_number - 1)}},
                "type": "enum",
                "enum": [0, 1, 3],
                "enum_titles": [
                    "Channel not Affected by Scene",
                    "Turn Off",
                    "Set Brightness"
                ],
                "group": "gg_ls_{{ls_number}}_ch_{{ch_number}}_action_on"
            },
            {
                "id": "ls_{{ls_number}}_ch_{{ch_number}}_fading_on",
                "title": "Dimming level rate (ms/%)",
                "reg_type": "holding",
                "address": {{1600 + (ls_number - 1) * 8 + (ch_number - 1)}},
                "max": 100,
                "required": true,
                "group": "gg_ls_{{ls_number}}_ch_{{ch_number}}_action_on",
                "condition": "ls_{{ls_number}}_ch_{{ch_number}}_action_on>=1"
            },
            {
                "id": "ls_{{ls_number}}_ch_{{ch_number}}_level_on",
                "title": "Brightness",
                "reg_type": "holding",
                "address": {{1664 + (ls_number - 1) * 8 + (ch_number - 1)}},
                "min": 1,
                "max": 100,
                "required": true,
                "group": "gg_ls_{{ls_number}}_ch_{{ch_number}}_action_on",
                "condition": "ls_{{ls_number}}_ch_{{ch_number}}_action_on==3"
            },
            {% endfor -%}
            {% endfor -%}

            {% for ch_number in range(1, OUTPUTS_NUMBER + 1) -%}
            {
                "id": "safety_mode_ch_{{ch_number}}_action",
                "title": "Channel {{ch_number}} Action",
                "reg_type": "holding",
                "address": {{16 + ch_number}},
                "type": "enum",
                "enum": [0, 1, 2, 3],
                "enum_titles": [
                    "No Action",
                    "Turn Off",
                    "Turn On",
                    "Turn On and Set Brightness"
                ],
                "group": "gg_safete_mode_ch_{{ ch_number }}"
            },
            {
                "id": "safety_mode_ch_{{ch_number}}_level",
                "title": "Channel {{ch_number}} Brightness",
                "reg_type": "holding",
                "address": {{16 + ch_number}},
                "max": 100,
                "required": true,
                "condition": "safety_mode_ch_{{ch_number}}_action==3",
                "group": "gg_safete_mode_ch_{{ ch_number }}"
            },
            {
                "id": "power_up_behaviour_ch_{{ch_number}}_action",
                "title": "Action on Power Up",
                "reg_type": "holding",
                "address": {{16 + ch_number}},
                "type": "enum",
                "enum": [0, 1, 2, 3],
                "enum_titles": [
                    "Turn Off",
                    "Set Previous State",
                    "Turn On",
                    "Turn On and Set Brightness"
                ],
                "group": "gg_power_up_behaviour_ch_{{ ch_number }}"
            },
            {
                "id": "power_up_behaviour_ch_{{ch_number}}_level",
                "title": "Brightness",
                "reg_type": "holding",
                "address": {{16 + ch_number}},
                "max": 100,
                "required": true,
                "condition": "power_up_behaviour_ch_{{ch_number}}_action==3",
                "group": "gg_power_up_behaviour_ch_{{ ch_number }}"
            },
            {% endfor -%}
            {% for in_number in range(1, INPUTS_NUMBER + 1) -%}
            {
                "id": "in_{{in_number}}_safety_input_control",
                "title": "Input {{in_number}}",
                "group": "gg_safety_input_control",
                "address": {{946 + in_number - 1}},
                "reg_type": "holding",
                "default": 0,
                "enum": [0, 1, 2],
                "enum_titles": [
                    "Don't change",
                    "Disable in safety mode",
                    "Enable only in safety mode"
                ]
            },
            {% endfor -%}
            {
                "id": "baud_rate",
                "title": "Baud rate",
                "description": "baud_rate_description",
                "address": 110,
                "reg_type": "holding",
                "enum": [96, 192, 384, 576, 1152],
                "default": 96,
                "enum_titles": [
                    "9600",
                    "19200",
                    "38400",
                    "57600",
                    "115200"
                ],
                "group": "general",
                "order": 1
            },
            {
                "id": "disable_indication",
                "title": "Status LED",
                "address": 130,
                "reg_type": "holding",
                "enum": [0, 1],
                "enum_titles": ["Enabled", "Disabled"],
                "default": 0,
                "group": "general"
            }
        ],
        "channels": [
            {% for index in range(0, INPUTS_NUMBER) -%}
            {
                "name": "Input {{index + 1}}",
                "reg_type": "discrete",
                "address": {{index}},
                "type": "switch",
                "group": "input_{{index + 1}}_counters"
            },
            {
                "name": "Input {{index + 1}} Counter",
                "reg_type": "input",
                "address": {{32 + index}},
                "type": "value",
                "sporadic": true,
                "enabled": true,
                "group": "input_{{index + 1}}_counters"
            },
            {
                "name": "Input {{index + 1}} Short Press Counter",
                "reg_type": "input",
                "address": {{464 + index}},
                "type": "value",
                "sporadic": true,
                "group": "input_{{index + 1}}_counters"
            },
            {
                "name": "Input {{index + 1}} Long Press Counter",
                "reg_type": "input",
                "address": {{480 + index}},
                "type": "value",
                "sporadic": true,
                "group": "input_{{index + 1}}_counters"
            },
            {
                "name": "Input {{index + 1}} Double Press Counter",
                "reg_type": "input",
                "address": {{496 + index}},
                "type": "value",
                "sporadic": true,
                "group": "input_{{index + 1}}_counters"
            },
            {
                "name": "Input {{index + 1}} Short Long Press Counter",
                "reg_type": "input",
                "address": {{512 + index}},
                "type": "value",
                "sporadic": true,
                "group": "input_{{index + 1}}_counters"
            },
            {% endfor -%}
            {% for index in range(0, OUTPUTS_NUMBER) -%}
            {
                "name": "Channel {{index + 1}} Switch",
                "reg_type": "coil",
                "address": {{16 + index}},
                "type": "switch",
                "sporadic": true,
                "group": "output_channels"
            },
            {
                "name": "Channel {{index + 1}} Dimming Level",
                "reg_type": "holding",
                "address": {{16 + index}},
                "type": "range",
                "sporadic": true,
                "max": 100,
                "group": "output_channels"
            },
            {
                "name": "Channel {{index + 1}}",
                "reg_type": "holding",
                "address": {{index}},
                "type": "range",
                "max": 10000,
                "enabled": false,
                "group": "output_channels"
            },
            {% endfor -%}
            {% for ls_number in range(1, LIGHT_SCENES_NUMBER + 1) -%}
            {
                "name": "Set Scene {{ls_number}}",
                "reg_type": "coil",
                "address": {{ 32 + (ls_number - 1)}},
                "type": "pushbutton",
                "enabled": false,
                "group": "gg_ls_{{ls_number}}_channels"
            },
            {% endfor -%}
            {
                "name": "Serial",
                "type": "text",
                "reg_type": "input",
                "address": 270,
                "format": "u32",
                "group": "hw_info"
            },
            {
                "name": "FW Version",
                "reg_type": "input",
                "address": 250,
                "type": "text",
                "format": "string",
                "string_data_size": 16,
                "enabled": false,
                "group": "hw_info"
            },
            {
                "name": "Supply voltage",
                "reg_type": "input",
                "address": 121,
                "type": "voltage",
                "scale": 0.001,
                "enabled": false,
                "group": "hw_info"
            },
            {
                "name": "Uptime",
                "reg_type": "input",
                "address": 104,
                "type": "text",
                "format": "u32",
                "enabled": false,
                "group": "hw_info"
            },
            {
                "name": "MCU Temperature",
                "reg_type": "input",
                "address": 124,
                "type": "temperature",
                "format": "s16",
                "scale": 0.1,
                "enabled": false,
                "group": "hw_info"
            },
            {
                "name": "MCU Voltage",
                "reg_type": "input",
                "address": 123,
                "scale": 0.001,
                "type": "voltage",
                "enabled": false,
                "group": "hw_info"
            },
            {% for index in range(0, OUTPUTS_NUMBER) -%}
            {
                "name": "Channel {{index + 1}} Duty Cycle Raw",
                "reg_type": "holding",
                "address": {{90 + index}},
                "type": "value",
                "readonly": true,
                "enabled": false,
                "group": "debug"
            }{% if not loop.last %},{% endif %}
            {% endfor -%}
        ],
        "translations": {
            "en": {
                "template_title": "{{ title_en }}",
                "pwm_frequency_factor_description": "PWM frequency is calculated by formula: 24 kHz / Factor ",
                "baud_rate_description": "Make sure that communication with device is established before changing this parameter. Select required baud rate, save configuration and then change port baud rate to the same value.",
                "Uptime": "Uptime (s)",

                {% for ch_number in range(1, OUTPUTS_NUMBER + 1) -%}
                "Channel {{ch_number}} Switch": "Channel {{ch_number}}",
                "Channel {{ch_number}}": "Channel {{ch_number}} Voltage",
                {% endfor -%}

