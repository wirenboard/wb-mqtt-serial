                "address": 25,
                "type": "switch"
            },
            {
                "name": "Opened door alarm",
                "group": "alarms",
                "reg_type": "discrete",
                "address": 28,
                "type": "switch"
            },

            {
                "name": "Compressor relay status",
                "reg_type": "discrete",
                "group": "status",
                "address": 0,
                "type": "switch"
            },
            {
                "name": "Defrost relay status",
                "group": "status",
                "reg_type": "discrete",
                "address": 13,
                "type": "switch"
            },
            {
                "name": "Fan relay status",
                "group": "status",
                "reg_type": "discrete",
                "address": 8,
                "type": "switch"
            },
            {
                "name": "Auxiliary relay status",
                "group": "status",
                "reg_type": "discrete",
                "address": 11,
                "type": "switch"
            },
            {
                "name": "Defrost status",
                "group": "status",
                "reg_type": "discrete",
                "address": 29,
                "type": "switch"
            },
            {
                "name": "Digital input status",
                "reg_type": "discrete",
                "group": "status",
                "address": 7,
                "type": "switch"
            },
            {
                "name": "Continuous cycle status",
                "reg_type": "discrete",
                "group": "status",
                "address": 27,
                "type": "switch"
            },
            {
                "name": "Carel serial address",
                "reg_type": "input",
                "group": "status",
                "address": 420,
                "type": "value"
            },
            {
                "name": "Defrost command",
                "reg_type": "coil",
                "group": "control",
                "address": 24,
                "type": "switch"
            },
            {
                "name": "OFF-ON command",
                "reg_type": "coil",
                "group": "control",
                "address": 31,
                "type": "switch",
                "on_value": 0,
                "off_value": 1
            },
            {
                "name": "Continuous cycle command",
                "reg_type": "coil",
                "group": "control",
                "address": 20,
                "type": "switch"
            },

            {
                "name": "Probe 1 reading",
                "group": "sensors",
                "reg_type": "input",
                "address": 405,
                "type": "temperature",
                "scale": 0.1,
                "format": "s16"
            },
            {
                "name": "Probe 2 reading",
                "group": "sensors",
                "reg_type": "input",
                "address": 409,
                "type": "temperature",
                "scale": 0.1,
                "format": "s16"
            },
            {
                "name": "Probe 3 reading",
                "group": "sensors",
                "reg_type": "input",
                "address": 406,
                "type": "temperature",
                "scale": 0.1,
                "format": "s16"
            },
            {
                "name": "Set Point",
                "group": "setpoint",
                "reg_type": "holding",
                "address": 410,
                "type": "temperature",
                "scale": 0.1,
                "round_to": 0.1,
                "format": "s16"
            },
            {
                "name": "HW Batch Number",
                "reg_type": "input",
                "address": 304,
                "type": "text",
                "format": "string",
                "string_data_size": 16,
                "group": "hw_info",
                "enabled": false
            }
        ]
    }
}
