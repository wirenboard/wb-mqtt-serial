{
    "device_type": "vakio-city-air-500",
    "title": "vakio_city_air_title",
    "group": "g-climate-control",
    "device": {
        "name": "Vakio City Air 250(500)",
        "id": "vakio_city_air_500",
        "groups": [
            {
                "title": "general",
                "id": "general",
                "order": 0
            },
            {
                "title": "setpoints",
                "id": "setpoints",
                "order": 1
            },
            {
                "title": "inputs",
                "id": "inputs",
                "order": 2
            },            
            {
                "title": "errors",
                "id": "errors",
                "order": 3
            }
        ],        
        "channels": [
            {
                "name": "state",
                "reg_type": "coil",
                "address": 0,
                "type": "switch",
                "group": "general"
            },
            {
                "name": "mode",
                "reg_type": "coil",
                "address": 1,
                "type": "value",
                "enum": [0, 1],
                "enum_titles": ["inflow", "smart"],
                "readonly": true,
                "group": "general"
            },
            {
                "name": "air_heater",
                "reg_type": "coil",
                "address": 3,
                "type": "switch",
                "group": "general"
            },
            {
                "name": "fan_speed_set",
                "reg_type": "holding",
                "address": 0,
                "type": "range",
                "min": 1,
                "max": 7,
                "group": "setpoints"
            },
            {
                "name": "fan_speed_current",
                "reg_type": "holding",
                "address": 3,
                "type": "value",
                "readonly": true,
                "group": "general"
            },
            {
                "name": "incoming_air_temperature",
                "reg_type": "holding",
                "address": 4,
                "type": "value",
                "readonly": true,
                "group": "inputs"
            },
            {
                "name": "heating_temperature_set",
                "reg_type": "holding",
                "address": 1,
                "type": "range",
                "min": 10,
                "max": 25,
                "group": "setpoints"
            },
            {
                "name": "air_damper_position",
                "reg_type": "holding",
                "address": 2,
                "type": "value",
                "enum": [0, 1, 2, 3],
                "enum_titles": ["close", "open", "opening", "closing"],
                "readonly": true,
                "group": "general"
            },
            {
                "name": "error_temperature_sensor",
                "reg_type": "coil",
                "address": 4,
                "type": "value",
                "enum": [0, 1],
                "enum_titles": ["no", "yes"],
                "readonly": true,
                "group": "errors"
            },
            {
                "name": "error_fan",
                "reg_type": "coil",
                "address": 5,
                "type": "value",
                "enum": [0, 1],
                "enum_titles": ["no", "yes"],
                "readonly": true,
                "group": "errors"
            },
            {
                "name": "error_underheating",
                "reg_type": "coil",
                "address": 6,
                "type": "value",
                "enum": [0, 1],
                "enum_titles": ["no", "yes"],
                "readonly": true,
                "group": "errors"
            },
            {
                "name": "error_overheating",
                "reg_type": "coil",
                "address": 7,
                "type": "value",
                "enum": [0, 1],
                "enum_titles": ["no", "yes"],
                "readonly": true,
                "group": "errors"
            },
            {
                "name": "error_low_incoming_temperature",
                "reg_type": "coil",
                "address": 8,
                "type": "value",
                "enum": [0, 1],
                "enum_titles": ["no", "yes"],
                "readonly": true,
                "group": "errors"
            },
            {
                "name": "high_outlet_temperature_error",
                "reg_type": "coil",
                "address": 9,
                "type": "value",
                "enum": [0, 1],
                "enum_titles": ["no", "yes"],
                "readonly": true,
                "group": "errors"
            },
            {
                "name": "fire_alarm",
                "reg_type": "coil",
                "address": 10,
                "type": "value",
                "enum": [0, 1],
                "enum_titles": ["no", "yes"],
                "readonly": true,
                "group": "inputs"
            }           
        ],
        "parameters": [
            {
                "id": "air_damper_exists",
                "title": "air_damper_exists",
                "reg_type": "coil",
                "address": 2,
                "format": "s8",
                "enum": [
                    0,
                    1
                ],
                "enum_titles": [
                    "not_installed",
                    "installed"
                    
                ],
                "default": 0,
                "group": "general"
            }
        ],        
