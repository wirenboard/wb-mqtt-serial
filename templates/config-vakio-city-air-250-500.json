{
    "device_type": "Vakio City Air 250(500)",
    "title": "vakio_city_air_title",
    "group": "g-climate-control",
    "device": {
        "name": "Vakio City Air 250(500)",
        "id": "vakio_city_air_500",
        "groups": [
            {
                "title": "general",
                "id": "general",
                "order": 0
            },
            {
                "title": "setpoints",
                "id": "setpoints",
                "order": 1
            },
            {
                "title": "sensors",
                "id": "sensors",
                "order": 2
            },            
            {
                "title": "alarms",
                "id": "alarms",
                "order": 3
            }
        ],        
        "channels": [
            {
                "name": "state",
                "reg_type": "coil",
                "address": 0,
                "type": "switch",
                "group": "general"
            },
            {
                "name": "mode",
                "reg_type": "coil",
                "address": 1,
                "type": "value",
                "enum": [0, 1],
                "enum_titles": ["inflow", "smart"],
                "readonly": true,
                "group": "general"
            },
            {
                "name": "air_heater",
                "reg_type": "coil",
                "address": 3,
                "type": "switch",
                "group": "general"
            },
            {
                "name": "fan_speed_setpoint",
                "reg_type": "holding",
                "address": 0,
                "type": "range",
                "min": 1,
                "max": 7,
                "group": "setpoints"
            },
            {
                "name": "fan_speed_current",
                "reg_type": "holding",
                "address": 3,
                "type": "value",
                "readonly": true,
                "group": "sensors"
            },
            {
                "name": "incoming_air_temperature",
                "reg_type": "holding",
                "address": 4,
                "type": "value",
                "readonly": true,
                "group": "sensors"
            },
            {
                "name": "heating_temperature_setpoint",
                "reg_type": "holding",
                "address": 1,
                "type": "range",
                "min": 10,
                "max": 25,
                "group": "setpoints"
            },
            {
                "name": "air_damper_position",
                "reg_type": "holding",
                "address": 2,
                "type": "value",
                "enum": [0, 1, 2, 3],
                "enum_titles": ["closed", "open", "opening", "closing"],
                "readonly": true,
                "group": "sensors",
                "condition": "isDefined(air_damper_exists)&&(air_damper_exists==1)"
            },
            {
                "name": "alarm_temperature_sensor",
                "reg_type": "coil",
                "address": 4,
                "type": "value",
                "enum": [0, 1],
                "enum_titles": ["no", "yes"],
                "readonly": true,
                "group": "alarms"
            },
            {
                "name": "alarm_fan",
                "reg_type": "coil",
                "address": 5,
                "type": "value",
                "enum": [0, 1],
                "enum_titles": ["no", "yes"],
                "readonly": true,
                "group": "alarms"
            },
            {
                "name": "alarm_underheating",
                "reg_type": "coil",
                "address": 6,
                "type": "value",
                "enum": [0, 1],
                "enum_titles": ["no", "yes"],
                "readonly": true,
                "group": "alarms"
            },
            {
                "name": "alarm_overheating",
                "reg_type": "coil",
                "address": 7,
                "type": "value",
                "enum": [0, 1],
                "enum_titles": ["no", "yes"],
                "readonly": true,
                "group": "alarms"
            },
            {
                "name": "alarm_low_incoming_temperature",
                "reg_type": "coil",
                "address": 8,
                "type": "value",
                "enum": [0, 1],
                "enum_titles": ["no", "yes"],
                "readonly": true,
                "group": "alarms"
            },
            {
                "name": "alarm_high_outlet_temperature",
                "reg_type": "coil",
                "address": 9,
                "type": "value",
                "enum": [0, 1],
                "enum_titles": ["no", "yes"],
                "readonly": true,
                "group": "alarms"
            },
            {
                "name": "alarm_fire",
                "reg_type": "coil",
                "address": 10,
                "type": "value",
                "enum": [0, 1],
                "enum_titles": ["no", "yes"],
                "readonly": true,
                "group": "alarms"
            }           
        ],
        "parameters": [
            {
                "id": "air_damper_exists",
                "title": "air_damper_exists",
                "reg_type": "coil",
                "address": 2,
                "format": "s8",
                "enum": [
                    0,
                    1
                ],
                "enum_titles": [
                    "not_installed",
                    "installed"
                    
                ],
                "default": 0,
                "group": "general"
            }
        ],        
