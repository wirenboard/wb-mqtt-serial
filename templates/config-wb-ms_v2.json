{
    "title": "WB-MS-v2_template_title",
    "device_type": "tpl1_wb_ms_v2",
    "group": "g-wb",
    "hw": [
        {
            "signature": "WBMSv2"
        }
    ],
    "device": {
        "name": "WB-MS v.2",
        "id": "wb-ms",
        "max_read_registers": 0,
        "response_timeout_ms": 1,
        "frame_timeout_ms": 12,
        "enable_wb_continuous_read": true,

        "groups": [
            {
                "title": "Input 1",
                "id": "input1"
            },
            {
                "title": "Input 2",
                "id": "input2"
            },
            {
                "title": "Air Quality",
                "id": "air_quality"
            },
            {
                "title": "Illuminance",
                "id": "illuminance"
            },
            {
                "title": "General",
                "id": "general"
            },
            {
                "title": "HW Info",
                "id": "hw_info"
            },
            {
                "title": "Debug",
                "id": "debug"
            }
        ],

        "parameters": {
            "th_sensor_heater_enabled": {
                "title": "TH Sensor Heater Enabled",
                "description": "Only for SHT40 temperature and humidity sensor",
                "address": 2,
                "reg_type": "coil",
                "enum": [0, 1],
                "enum_titles": ["no", "yes"],
                "default": 0,
                "group": "air_quality",
                "order": 1
            },
            "selfheating_compensation_deg": {
                "title": "Self-Heating Compensation (°C)",
                "description": "selfheating_compensation_deg_description",
                "address": 245,
                "reg_type": "holding",
                "format": "s16",
                "scale": 0.01,
                "group": "air_quality",
                "order": 2
            },
            "illuminance_filter_time_const_ms": {
                "title": "Low-pass Filter Time Constant (ms)",
                "address": 90,
                "reg_type": "holding",
                "scale": 0.1,
                "default": 20,
                "group": "illuminance",
                "order": 1
            },
            "baud_rate": {
                "title": "Baud rate",
                "description": "baud_rate_description",
                "address": 110,
                "reg_type": "holding",
                "enum": [96, 192, 384, 576, 1152],
                "default": 96,
                "enum_titles": [
                    "9600",
                    "19200",
                    "38400",
                    "57600",
                    "115200"
                ],
                "group": "general",
                "order": 1
            },
            "temperature_readings_filter_deg": {
                "title": "temperature_readings_filter_deg_title",
                "description": "temperature_readings_filter_deg_description",
                "address": 99,
                "reg_type": "holding",
                "scale": 0.0625,
                "default": 1,
                "group": "general",
                "order": 2
            },
            "rs485_response_delay_ms": {
                "title": "RS-485 Response Delay (ms)",
                "address": 113,
                "reg_type": "holding",
                "default": 8,
                "group": "general",
                "order": 3
            },
            "disable_indication": {
                "title": "Status LED",
                "address": 130,
                "reg_type": "holding",
                "enum": [0, 1],
                "enum_titles": ["Enabled", "Disabled"],
                "default": 0,
                "group": "general",
                "order": 4
            },
            "input1_type": {
                "title": "Input Type",
                "address": 275,
                "reg_type": "holding",
                "enum": [0, 1],
                "enum_titles": ["1-Wire", "Discrete Input"],
                "default": 0,
                "group": "input1"
            },
            "input2_type": {
                "title": "Input Type",
                "address": 276,
                "reg_type": "holding",
                "enum": [0, 1],
                "enum_titles": ["1-Wire", "Discrete Input"],
                "default": 0,
                "group": "input2"
            }
        },

        "channels": [
            {
                "name": "Temperature",
                "reg_type": "input",
                "address": 4,
                "type": "temperature",
                "format": "s16",
                "scale": 0.01,
                "error_value": "0x7FFF",
                "group": "air_quality"
            },
            {
                "name": "Humidity",
                "reg_type": "input",
                "address": 5,
                "type": "rel_humidity",
                "format": "s16",
                "scale": 0.01,
                "error_value": "0xFFFF",
                "group": "air_quality"
            },
            {
                "name": "Air Quality (VOC)",
                "reg_type": "input",
                "address": 11,
                "units": "ppb",
                "error_value": "0xFFFF",
                "group": "air_quality"
            },
            {
                "name": "Air Quality Index",
                "reg_type": "input",
                "address": 12,
                "scale": 1,
                "error_value": "0xFFFF",
                "unsupported_value": "0xFFFE",
                "enabled": false,
                "group": "air_quality"
            },
            {
                "name": "TH Sensor Heater Cleaning Mode Enabled",
                "description": "Only for SHT40 temperature and humidity sensor",
                "address": 8,
                "fw": "4.35.0",
                "reg_type": "coil",
                "type": "switch",
                "enabled": false,
                "group": "air_quality"
            },
            {
                "name": "TH Sensor Heater Cleaning Mode Countdown",
                "description": "Only for SHT40 temperature and humidity sensor",
                "address": 975,
                "fw": "4.35.0",
                "reg_type": "input",
                "type": "value",
                "units": "s",
                "format": "u16",
                "enabled": false,
                "group": "air_quality"
            },
            {
                "name": "Illuminance",
                "reg_type": "input",
                "address": 2,
                "type": "lux",
                "group": "illuminance"
            },
            {
                "name": "Serial",
                "reg_type": "input",
                "address": 270,
                "type": "text",
                "format": "u32",
                "group": "hw_info"
            },
            {
                "name": "FW Version",
                "reg_type": "input",
                "address": 250,
                "type": "text",
                "format": "string",
                "string_data_size": 16,
                "enabled": false,
                "group": "hw_info"
            },
            {
                "name": "Input Voltage",
                "reg_type": "input",
                "address": 121,
                "scale": 0.001,
                "type": "voltage",
                "enabled": false,
                "group": "hw_info"
            },
            {
                "name": "Uptime",
                "reg_type": "input",
                "address": 104,
                "format": "u32",
                "enabled": false,
                "group": "hw_info"
            },
            {
                "name": "Minimum Voltage Since Startup",
                "reg_type": "input",
                "address": 122,
                "scale": 0.001,
                "type": "voltage",
                "enabled": false,
                "group": "hw_info"
            },
            {
                "name": "MCU Temperature",
                "reg_type": "input",
                "address": 124,
                "type": "temperature",
                "format": "s16",
                "scale": 0.1,
                "enabled": false,
                "group": "hw_info"
            },
            {
                "name": "MCU Voltage",
                "reg_type": "input",
                "address": 123,
                "scale": 0.001,
                "type": "voltage",
                "enabled": false,
                "group": "hw_info"
            },
            {
                "name": "VOC Sensor Installed",
                "reg_type": "input",
                "address": 371,
                "type": "switch",
                "enabled": false,
                "group": "debug"
            },
            {
                "name": "VOC Version",
                "reg_type": "input",
                "address": 108,
                "error_value": "0xFFFF",
                "enabled": false,
                "group": "debug"
            },
            {
                "name": "VOC Raw",
                "reg_type": "input",
                "address": 107,
                "error_value": "0xFFFF",
                "enabled": false,
                "group": "debug"
            },
            {
                "name": "VOC Raw R4",
                "reg_type": "input",
                "address": 364,
                "format": "u16",
                "enabled": false,
                "group": "debug"
            },
            {
                "name": "VOC Baseline",
                "reg_type": "input",
                "address": 106,
                "enabled": false,
                "group": "debug"
            },
            {
                "name": "VOC Equivalent CO2 Concentration",
                "reg_type": "input",
                "address": 360,
                "format": "u16",
                "error_value": "0xFFFF",
                "unsupported_value": "0xFFFE",
                "units": "ppm",
                "enabled": false,
                "group": "debug"
            },
            {
                "name": "VOC Sensor Status",
                "reg_type": "input",
                "address": 363,
                "format": "u16",
                "error_value": "0xFFFF",
                "unsupported_value": "0xFFFE",
                "enabled": false,
                "group": "debug"
            },
            {
                "name": "Illuminance Mode",
                "reg_type": "holding",
                "address": 103,
                "readonly": true,
                "enabled": false,
                "group": "debug"
            },
            {
                "name": "Successful TH Readings",
                "reg_type": "input",
                "address": 101,
                "enabled": false,
                "group": "debug"
            },
            {
                "name": "Erroneous TH Readings",
                "reg_type": "input",
                "address": 102,
                "enabled": false,
                "group": "debug"
            },
            {
                "name": "External Sensor 1",
                "reg_type": "input",
                "address": "6",
                "type": "temperature",
                "format": "s16",
                "scale": 0.0625,
                "error_value": "0x7FFF",
                "condition": "input1_type==0",
                "group": "input1"
            },
            {
                "name": "Input 1",
                "reg_type": "discrete",
                "address": 0,
                "type": "switch",
                "condition": "input1_type==1",
                "group": "input1"
            },
            {
                "name": "Input 1 Counter",
                "reg_type": "input",
                "address": 277,
                "type": "value",
                "condition": "input1_type==1",
                "group": "input1"
            },
            {
                "name": "External Sensor 2",
                "reg_type": "input",
                "address": "7",
                "type": "temperature",
                "format": "s16",
                "scale": 0.0625,
                "error_value": "0x7FFF",
                "condition": "input2_type==0",
                "group": "input2"
            },
            {
                "name": "Input 2",
                "reg_type": "discrete",
                "address": 1,
                "type": "switch",
                "condition": "input2_type==1",
                "group": "input2"
            },
            {
                "name": "Input 2 Counter",
                "reg_type": "input",
                "address": 278,
                "type": "value",
                "condition": "input2_type==1",
                "group": "input2"
            }
        ],
