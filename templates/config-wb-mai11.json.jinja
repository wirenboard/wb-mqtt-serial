            {
                "title": "Channel P",
                "id": "gg_in{{ ch_num }}_p",
                "group": "g_in{{ ch_num }}"
            },
            {
                "title": "Channel N",
                "id": "gg_in{{ ch_num }}_n",
                "group": "g_in{{ ch_num }}"
            },
            {
                "title": "IN_P_channels",
                "id": "gg_in{{ ch_num }}_p_channels",
                "group": "gg_in{{ ch_num }}_p",
                "ui_options": {
                    "wb": {
                        "disable_title": true
                    }
