        "setup": [
{%- set separator = joiner(',') %}
{%- for ch_num in range(1, CHANNELS_NUMBER + 1) %}{{ separator() }}
            {
                "title": "Sampling time IN {{ ch_num }} P",
                "address": "0x{{ "%x"|format(ch_num) }}402",
                "value": 0,
                "condition": "in{{ ch_num }}_type==20000"
