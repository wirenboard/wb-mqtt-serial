                "scale": 1.52588e-05,
                "round_to": 0.001,
                "enabled": false,
                "group": "button_1"
            },
            {
                "name": "Button 2",
                "reg_type": "discrete",
                "address": 1,
                "type": "switch",
                "group": "button_2"
            },
            {
                "name": "Button 2 counter",
                "reg_type": "input",
                "address": 33,
                "type": "value",
                "group": "button_2"
            },
            {
                "name": "Button 2 freq",
                "reg_type": "input",
