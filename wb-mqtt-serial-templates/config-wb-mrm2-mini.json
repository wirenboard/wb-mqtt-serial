{
    "device_type": "WB-MRM2-mini",
    "device": {
        "name": "WB-MRM2-mini",
        "id": "wb-mrm2-mini",
        "max_read_registers": 0,
        "response_timeout_ms": 1,
        "frame_timeout_ms": 8,

        "groups": [
            {
                "title": "Button 1",
                "id": "button_1",
                "order": 1
            },
            {
                "title": "Button 2",
                "id": "button_2",
                "order": 2
            },
            {
                "title": "Outputs",
                "id": "outputs",
                "order": 3
            },
            {
                "title": "General Settings",
                "id": "general",
                "order": 4
            },
            {
                "title": "HW Info",
