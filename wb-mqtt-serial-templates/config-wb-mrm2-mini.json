                "enum": [ 0, 1 ],
                "default": 0,
                "enum_titles": [
                    "no",
                    "yes"
                ],
                "group": "outputs",
                "order": 1
            },
            "safety_timer_s": {
                "title": "Safety Timer (s)",
                "address": 8,
                "reg_type": "holding",
                "default": 3600,
                "group": "general",
                "order": 1
