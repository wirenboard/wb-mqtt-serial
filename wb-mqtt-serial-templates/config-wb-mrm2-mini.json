                "title": "Button 1 Mode",
                "address": 9,
                "reg_type": "holding",
                "enum": [ 0, 1, 2, 3, 4 ],
                "default": 0,
                "enum_titles": [
                    "push button",
                    "latching switch",
                    "turn off all outputs when activated",
                    "disable communication between input and outputs",
                    "operate according to mapping-matix"
                ],
                "group": "button_1",
                "order": 1
            },
            "button1_debounce_ms": {
                "title": "Button 1 Debounce (ms)",
                "address": 20,
                "reg_type": "holding",
                "min": 0,
                "max": 100,
                "default": 50,
                "group": "button_1",
                "order": 2
            },
            "button2_mode": {
                "title": "Button 2 Mode",
                "address": 10,
                "reg_type": "holding",
                "enum": [ 0, 1, 2, 3, 4 ],
                "default": 0,
                "enum_titles": [
                    "push button",
                    "latching switch",
                    "turn off all outputs when activated",
                    "disable communication between input and outputs",
                    "operate according to mapping-matix"
