          "0. NONE",
          "1. 2-pipe",
          "2. 2-pipe 3-position",
          "3. 2-pipe with electric heater",
          "4. 4-pipe"
        ],
        "default": 0
    },
    "control": {
        "order": 2,
        "title": "P01. Control sequence. 2-pipe: [0…3], 4-pipe: [2…4]",
