        "enum": [ 0, 1, 2, 3, 4 ],
        "enum_titles": [
          "0. Heating only",
          "1. Cooling only",
          "2. H/C changeover manual",
          "3. H/C changeover auto",
          "4. Heating and Cooling"
        ],
        "default": 1
    },
    "fahrenheit": {
        "order": 3,
        "title": "P04. Selection of °C or °F",
        "address": "0x0003",
        "enum": [ 0, 1 ],
        "enum_titles": [
          "0. °C",
          "1. °F"
        ],
        "default": 0
    }
  },
  "channels": [
